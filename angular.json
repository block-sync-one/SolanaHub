{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "app": {
      "projectType": "application",
      "schematics": {
        "@ionic/angular-toolkit:page": {
          "styleext": "scss",
          "standalone": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "./webpack.client.config.ts"
            },
            "outputPath": "www",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "assets"
              },
              "src/actions.json"
            ],
            "styles": ["src/global.scss", "src/theme/variables.scss", "node_modules/ngx-easy-table/style.css"],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "@marinade.finance/marinade-ts-sdk", "camelcase", "js-sha3", "bs58",
              "@walletconnect/qrcode-modal", "@ledgerhq/devices/lib/hid-framing",
              "bn.js", "borsh", "rpc-websockets/dist/lib/client/websocket.browser",
              "@toruslabs/eccrypto", "eth-rpc-errors", "fast-deep-equal", "pump",
              "@walletconnect/window-getters", "@walletconnect/window-metadata",
              "@sonarwatch/portfolio-core", "@coral-xyz/anchor/dist/cjs/coder/borsh/accounts",
              "lottie-web", "@solana/buffer-layout", "bigint-buffer",
              "jayson/lib/client/browser", "rpc-websockets/dist/lib/client",
              "is-stream", "loglevel", "@walletconnect/environment", "buffer",
              "sdp", "rtcpeerconnection-shim", "@stablelib/ed25519",
              "@walletconnect/time", "@coral-xyz/borsh", "eventemitter3",
              "@stablelib/base64", "ed2curve", "tweetnacl", "qrcode",
              "@babel/runtime/regenerator"
            ]
          }
        },
        "server": {
          "builder": "@angular-builders/custom-webpack:server",
          "options": {
            "customWebpackConfig": {
              "path": "./webpack.config.ts"
            },
            "outputPath": "dist/app/server",
            "main": "src/main.server.ts"
          }
        },
        "prerender": {
          "builder": "@angular-devkit/build-angular:prerender",
          "options": {
            "routes": ["/"],
            "serverTarget": "app:server",
            "browserTarget": "app:build"
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "app:build:production"
            },
            "development": {
              "buildTarget": "app:build:development"
            },
            "local": {
              "buildTarget": "app:build:local"
            },
            "ssr": {
              "buildTarget": "app:server"
            }
          },
          "defaultConfiguration": "development"
        },
        "serve-ssr": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/app/server",
            "main": "src/main.server.ts",
            "tsConfig": "tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "app:server:production"
            },
            "development": {
              "buildTarget": "app:server:development"
            }
          },
          "defaultConfiguration": "production"
        },
        "ssr": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/app/server",
            "main": "src/main.server.ts",
            "tsConfig": "tsconfig.server.json",
            "server": "src/server.ts",
            "prerender": true
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": ["@ionic/angular-toolkit"]
  },
  "schematics": {
    "@ionic/angular-toolkit:component": {
      "styleext": "scss"
    },
    "@ionic/angular-toolkit:page": {
      "styleext": "scss"
    },
    "@angular-eslint/schematics:application": {
      "setParserOptionsProject": true
    },
    "@angular-eslint/schematics:library": {
      "setParserOptionsProject": true
    }
  }
}
